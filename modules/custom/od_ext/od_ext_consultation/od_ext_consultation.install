/**
 * @file
 * Contains install and update routines for Open Data Consultation.
 */

/**
 * Implements hook_install().
 */
function od_ext_consultation_install() {
  // Enable translation for our core entities/bundles.
  $entities = [
    'node' => [
      'consultation',
    ],
  ];
  wxt_ext_translation_enable_translation($entities);
}
